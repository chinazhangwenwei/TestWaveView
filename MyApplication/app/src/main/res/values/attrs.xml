    <declare-styleable name="MyWaveView2">
        <attr name="wave_color1" format="color" />
        <attr name="wave_color2" format="color" />
        <attr name="paint_width" format="dimension" />
        <attr name="wave_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RaderView">
        <attr name="rader_paint_width" format="dimension" />
        <attr name="rader_color" format="color" />
        <attr name="rader_line_colro" format="color" />
        <attr name="rader_circle_count" format="integer" />
    </declare-styleable>

</resources>