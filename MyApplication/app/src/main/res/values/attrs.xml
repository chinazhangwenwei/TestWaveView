        <attr name="WaveViewType" format="integer">
            <enum name="Circle_Type" value="1" />
            <enum name="Rect_type" value="2" />
        </attr>
        <attr name="WaveSpeed" format="integer" />
        <attr name="WaveColor" format="color" />
        <attr name="WaveHeight" format="dimension" />
        <attr name="PaintWidth" format="dimension" />
        <attr name="TextSize" format="dimension" />
